node {
stage('Build') {
// Invoke the AWS CodeBuild
awsCodeBuild artifactLocationOverride: '', artifactNameOverride: '', artifactNamespaceOverride: '', artifactPackagingOverride: '', artifactPathOverride: '', artifactTypeOverride: '', awsAccessKey: env.AWS_ACCESS_KEY_ID, awsSecretKey: env.AWS_SECRET_ACCESS_KEY, buildSpecFile: '', buildTimeoutOverride: '', credentialsId: '', credentialsType: 'keys', envVariables: '', projectName: 'jenkins-build', proxyHost: '', proxyPort: '', region: 'ap-southeast-2', sourceVersion: '', sseAlgorithm: ''
}
}
